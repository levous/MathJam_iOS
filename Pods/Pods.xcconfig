PODS_ROOT = ${SRCROOT}/Pods
PODS_HEADERS_SEARCH_PATHS = ${PODS_PUBLIC_HEADERS_SEARCH_PATHS}
ALWAYS_SEARCH_USER_PATHS = YES
OTHER_LDFLAGS = -ObjC -lFlurry -framework CoreText -framework GLKit -framework OpenGLES -framework QuartzCore -framework SystemConfiguration
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/FlurrySDK/Flurry"
HEADER_SEARCH_PATHS = ${PODS_HEADERS_SEARCH_PATHS}
PODS_BUILD_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/BuildHeaders" "${PODS_ROOT}/BuildHeaders/FRD3DBarChart" "${PODS_ROOT}/BuildHeaders/FlurrySDK"
PODS_PUBLIC_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/FRD3DBarChart" "${PODS_ROOT}/Headers/FlurrySDK"